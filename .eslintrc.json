{
  "extends": "eslint:recommended",
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": { }
  },
  "plugins": [
    "typescript"
  ],
  "rules": {
    "no-undef": ["off"],
    "no-unused-vars": ["off"],
    "no-useless-escape": ["off"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "typescript/class-name-casing": "error"
  }
}