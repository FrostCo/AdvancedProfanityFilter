{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "lib": ["es2022", "dom", "dom.iterable"],
    "moduleResolution": "node",
    "outDir": "./built",
    "paths": {
      "@APF/*": ["src/script/*"],
      "@VENDOR/*": ["src/script/vendor/*"]
    },
    "skipLibCheck": true,
    "target": "esnext"
  },
  "include": ["./src/script/**/*.ts"]
}
